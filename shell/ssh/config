Include ~/.orbstack/ssh/config

Include ./home.config
Include ./cloud.config

Host *
  Protocol 2
  ServerAliveInterval 30
  ServerAliveCountMax 60
  TCPKeepAlive yes
  AddKeysToAgent yes
  UseKeychain yes
  StrictHostKeyChecking no
  IdentityFile ~/.ssh/id_rsa

Host github.com
  HostName ssh.github.com
  Port 443
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa
